n\r\n:root {\r\n  --font-xxl-h1: 3.75rem;\r\n  --font-xl-h2: 2.25rem;\r\n  --font-l-h3: 1.375rem;\r\n  --font-pera-p: 1.125rem;\r\n  --font-small-p: 1rem;\r\n  --font-extra-small-a-span: 0.9375rem;\r\n  --box-shadow: 0px 0px 2px rgba(0, 0, 0, 0.08),\r\n    0px 2px 10px rgba(0, 0, 0, 0.08), 0px 16px 20px rgba(0, 0, 0, 0.16);\r\n  --dark-violet-color: #562af0;\r\n  --light-violet-color: #4d3add;\r\n  --dark-sand-color: #464646;\r\n  --dark-blue-color: #0a0535;\r\n  --dark-color: #000000;\r\n  --light-color: #ffffff;\r\n}\r\n\r\n* {\r\n  margin: 0;\r\n  padding: 0;\r\n  box-sizing: border-box;\r\n}\r\n\r\nhtml {\r\n  scroll-behavior: smooth;\r\n}\r\n\r\nbody {\r\n  font-size: 1rem;\r\n  overflow-x: hidden;\r\n  font-family: \"Poppins\", sans-serif;\r\n}\r\n\r\nimg {\r\n  vertical-align: middle;\r\n}\r\n\r\ninput,\r\na,\r\nbutton {\r\n  border: none;\r\n  outline: none;\r\n  text-decoration: none;\r\n  position: relative;\r\n  overflow: hidden;\r\n  transition: all 0.5s ease;\r\n  font-size: var(--font-extra-small-a-span);\r\n  color: var(--dark-color);\r\n  text-transform: capitalize;\r\n}\r\n\r\n::after,\r\n::before {\r\n  box-sizing: border-box;\r\n  transition: all 0.5s ease;\r\n}\r\n\r\nul {\r\n  list-style: none;\r\n}\r\n\r\n/* utilities classes */\r\n\r\n.container {\r\n  max-width: 1300px;\r\n  margin: 0 auto;\r\n  overflow: hidden;\r\n}\r\n\r\n.row {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: space-between;\r\n  flex-wrap: wrap;\r\n}\r\n\r\n.section {\r\n  max-height: 300vh;\r\n  min-height: 100vh;\r\n  height: fit-content;\r\n  overflow: hidden;\r\n}\r\n\r\n.img-box {\r\n  overflow: hidden;\r\n  vertical-align: middle;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n\r\n.nav-btn {\r\n  font-family: \"Sora\", sans-serif;\r\n}\r\n\r\n/* navbar big screen */\r\n\r\nnav {\r\n  padding: 2rem 0;\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n  width: 100%;\r\n  height: auto;\r\n  transition: all 0.5s ease;\r\n  z-index: 100;\r\n}\r\n\r\nnav.active {\r\n  box-shadow: 0 0 20px #dddddd;\r\n  transition: all 0.5s ease;\r\n  padding: 1rem;\r\n}\r\n\r\nnav nav .container,\r\nnav .row {\r\n  width: 100%;\r\n}\r\n\r\nnav .nav-btns {\r\n  width: 15rem;\r\n}\r\n\r\nnav .nav-btns a {\r\n  padding: 0.4rem 0;\r\n}\r\n\r\nnav .nav-btns a::after {\r\n  position: absolute;\r\n  z-index: 2;\r\n  width: 0;\r\n  height: 2px;\r\n  background-color: var(--dark-violet-color);\r\n  content: \"\";\r\n  bottom: 0;\r\n  left: 0;\r\n}\r\n\r\nnav .nav-btns a:hover {\r\n  color: var(--dark-violet-color);\r\n}\r\n\r\nnav .nav-btns a:hover::after {\r\n  width: 100%;\r\n}\r\n\r\nnav .btn-2 {\r\n  color: var(--dark-violet-color);\r\n  border: 1px solid var(--dark-violet-color);\r\n  padding: 0.6rem 2rem;\r\n  border-radius: 5px;\r\n}\r\n\r\nnav .btn-2:hover {\r\n  background-color: var(--dark-violet-color);\r\n  color: var(--light-color);\r\n}\r\n\r\n/* banner */\r\n\r\n.banner {\r\n  width: 100%;\r\n  height: 100%;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.banner .container,\r\n.banner .row {\r\n  height: 100%;\r\n  background-color: red;\r\n  border-radius: 20px;\r\n}\r\n"},"lineCount":182}},"error":null,"hash":"9c83b54c26dbcf3ea6f163a0cba0a853","cacheData":{"env":{}}}