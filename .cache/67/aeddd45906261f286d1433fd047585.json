ginal":{"line":32,"column":3}},{"generated":{"line":44,"column":3},"source":"javascript/initPageTransitions.js","original":{"line":21,"column":2}},{"generated":{"line":45,"column":0},"source":"javascript/initPageTransitions.js","original":{"line":33,"column":1}},{"generated":{"line":45,"column":1},"source":"javascript/initPageTransitions.js","original":{"line":1,"column":0}},{"name":"initPageTransitions","generated":{"line":47,"column":15},"source":"javascript/initPageTransitions.js","original":{"line":35,"column":15}}],"sources":{"javascript/initPageTransitions.js":"const initPageTransitions = () => {\r\n  setTimeout(() => document.body.classList.add(\"render\"), 60);\r\n  const navdemos = Array.from(document.querySelectorAll(\".demos__links .demo\"));\r\n  const total = navdemos.length;\r\n  const current = navdemos.findIndex((el) =>\r\n    el.classList.contains(\"demo--current\")\r\n  );\r\n  const navigate = (linkEl) => {\r\n    document.body.classList.remove(\"render\");\r\n    document.body.addEventListener(\r\n      \"transitionend\",\r\n      () => (window.location = linkEl.href)\r\n    );\r\n  };\r\n  navdemos.forEach((link) =>\r\n    link.addEventListener(\"click\", (ev) => {\r\n      ev.preventDefault();\r\n      navigate(ev.currentTarget);\r\n    })\r\n  );\r\n  document.addEventListener(\"keydown\", (ev) => {\r\n    const keyCode = ev.keyCode || ev.which;\r\n    let linkEl;\r\n    if (keyCode === 37) {\r\n      linkEl = current > 0 ? navdemos[current - 1] : navdemos[total - 1];\r\n    } else if (keyCode === 39) {\r\n      linkEl = current < total - 1 ? navdemos[current + 1] : navdemos[0];\r\n    } else {\r\n      return false;\r\n    }\r\n    navigate(linkEl);\r\n  });\r\n};\r\n\r\nexport default initPageTransitions;\r\n"},"lineCount":null}},"error":null,"hash":"355129ceb3a5120ea157431d82e45672","cacheData":{"env":{}}}