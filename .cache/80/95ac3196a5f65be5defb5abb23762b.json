ay: block;\r\n  text-transform: uppercase;\r\n  white-space: nowrap;\r\n  transition: all 0.5s ease;\r\n}\r\n\r\n.hover-reveal {\r\n  position: absolute;\r\n  z-index: -1;\r\n  width: 500px;\r\n  height: 280px;\r\n  top: 0;\r\n  left: 0;\r\n  pointer-events: none;\r\n  opacity: 0;\r\n  will-change: transform, filter;\r\n}\r\n\r\n.hover-reveal__inner {\r\n  box-shadow: var(--box-shadow);\r\n  border-radius: 10px;\r\n  border: 10px solid #f4f4f4;\r\n  overflow: hidden;\r\n}\r\n\r\n.hover-reveal__inner,\r\n.hover-reveal__img {\r\n  width: 100%;\r\n  height: 100%;\r\n  position: relative;\r\n}\r\n\r\n.hover-reveal__img {\r\n  background-size: cover;\r\n  background-position: 50% 50%;\r\n}\r\n\r\n/* footer */\r\nfooter {\r\n  border-top: 2px dashed violet;\r\n}\r\n"},"lineCount":176}},"error":null,"hash":"931af89b6ac9fb00a3bb5cb87fffe031","cacheData":{"env":{}}}